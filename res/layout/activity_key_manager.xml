<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/keyManagerLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="20dp"
    tools:context="org.sector67.nsaaway.KeyManagerActivity"
    tools:ignore="MergeRootFrame" >

    <Button
        android:id="@+id/createTestKeysButton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="@string/create_test_keys_button" />
     <Button
        android:id="@+id/listKeysButton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/createTestKeysButton"
        android:layout_centerHorizontal="true"
        android:layout_margin="20dp"
        android:text="@string/list_keys_button" />   
</RelativeLayout>
